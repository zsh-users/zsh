  _arguments \
    '(-h --heads)'{-h,--heads}'[show only refs under refs/heads]' \
    '(-t --tags)'{-t,--tags}'[show only refs under refs/tags]' \
    $upload_pack_arg \
    ':repository:__git_any_repositories' \
    '*: :__git_references' && ret=0
}

